{
	"format_version": "1.8.0",
	"animations": {
		"animation.chicken.general.v1.0": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": ["90.0 - this", 0, 0]
				},
				"wing0": {
					"rotation": [0, 0, "variable.wing_flap - this"]
				},
				"wing1": {
					"rotation": [0, 0, "-variable.wing_flap - this"]
				}
			}
		},
		"animation.chicken.baby_transform": {
			"loop": true,
			"bones": {
				"head": {
					"scale": 2
				}
			}
		},
		"animation.chicken.general": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": ["-this", 0, 0]
				},
				"wing0": {
					"rotation": [0, 0, "variable.wing_flap - this"]
				},
				"wing1": {
					"rotation": [0, 0, "-variable.wing_flap - this"]
				}
			}
		},
		"animation.chicken.move": {
			"loop": true,
			"anim_time_update": "query.modified_distance_moved",
			"bones": {
				"leg0": {
					"rotation": ["math.cos(query.anim_time * 38.17) *  80.0", 0, 0]
				},
				"leg1": {
					"rotation": ["math.cos(query.anim_time * 38.17) * -80.0", 0, 0]
				},
				"left_arm": {
					"rotation": ["math.cos(query.anim_time * 38.17) *  80.0", 0, 0]
				},
				"right_arm": {
					"rotation": ["math.cos(query.anim_time * 38.17) * -80.0", 0, 0]
				}
			}
		}
	}
}